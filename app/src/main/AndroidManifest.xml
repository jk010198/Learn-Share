<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ithub.com.blogposting">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".OtpVerfication" />
        <activity android:name=".AddBlogPage" />
        <activity android:name=".AdminHome" />
        <activity android:name=".FullCodeView" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WebActivityComplier" />
        <activity android:name=".MyBlogList" />
        <activity android:name=".MyBlogDetails" />
        <activity android:name=".AdminAllBlogList" />
        <activity android:name=".ApproveBlogActivity" />
        <activity android:name=".AdminBlogDetails"></activity>
    </application>

</manifest>